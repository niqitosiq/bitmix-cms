{
    "name": "backend",
    "type": "module",
    "version": "1.0.0",
    "description": "Express app with Prisma ORM using SQLite database",
    "main": "src/app.ts",
    "scripts": {
        "start": "node -r dotenv/config src/app.js",
        "dev": "tsx watch src/app.ts",
        "generate": "npx prisma generate --schema ./src/prisma/schema.prisma",
        "migrate": "npx prisma migrate dev --schema ./src/prisma/schema.prisma",
        "library": "tsx ./generator/index.ts"
    },
    "keywords": [
        "express",
        "prisma",
        "sqlite"
    ],
    "author": "Your Name",
    "license": "MIT",
    "dependencies": {
        "@prisma/client": "^5.16.1",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "prisma": "^5.16.1",
        "random-words": "^2.0.1",
        "sqlite3": "^5.0.2",
        "ts-morph": "^23.0.0",
        "uuid": "^10.0.0",
        "yawg": "^1.2.3"
    },
    "devDependencies": {
        "@types/express": "^4.17.21",
        "@types/uuid": "^10.0.0",
        "nodemon": "^2.0.12",
        "tsx": "^4.16.2",
        "typescript": "^4.4.3"
    }
}
